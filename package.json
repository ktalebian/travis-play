{
  "name": "travis-play",
  "private": true,
  "description": "playing with travis",
  "homepage": "https://github.com/ktalebian/travis-play#readme",
  "bugs": {
    "url": "https://github.com/ktalebian/travis-play/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ktalebian/travis-play.git"
  },
  "license": "ISC",
  "author": "",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap --no-ci",
    "build": "lerna run build",
    "postinstall": "npm run bootstrap",
    "lint": "echo 'no test'",
    "publish": "lerna-travis-publish",
    "relea2se": "lerna version minor --yes --no-push",
    "foo": "$(git describe --tags --abbrev=0 | cut -c 2-)",
    "postrelease": "dotenv release-it -- $(git describe --tags --abbrev=0 | cut -c 2-) --ci",
    "release": "lerna-travis-release $1 $2",
    "release:alpha": "npm run release alpha $1",
    "release:beta": "npm run release beta $1",
    "release:public": "npm run release public $1",
    "test": "echo 'no test'"
  },
  "devDependencies": {
    "@k88/lerna-travis": "2.0.1",
    "@types/node": "^14.14.13",
    "auto-changelog": "^2.2.1",
    "dotenv-cli": "^4.0.0",
    "lerna": "^3.22.1",
    "release-it": "^14.2.2",
    "typescript": "^4.1.3"
  },
  "version": "4.19.0"
}
